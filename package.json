{
  "name": "rssfeed-metro-tile",
  "version": "2.1.0",
  "description": "RSS Feed Metro Tile for Home Assistant",
  "type": "module",
  "main": "dist/rssfeed-metro-tile.js",
  "scripts": {
    "dev": "vite",
    "dev:hmr": "concurrently \"npm run build:watch\" \"npm run dev:vite\" --names \"build,vite\" --prefix-colors \"blue,green\"",
    "dev:vite": "vite",
    "build:watch": "rollup -c -w",
    "build": "rollup -c",
    "build:dev": "rollup -c --environment DEV",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/state": "^6.5.4",
    "@codemirror/view": "^6.39.11",
    "codemirror": "^6.0.2",
    "concurrently": "^9.2.1",
    "custom-card-helpers": "^1.9.0",
    "js-yaml": "^4.1.1",
    "lit": "^3.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.4.21",
    "vitest": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/florianschmidt/ha-rssfeedMetroTile"
  },
  "keywords": [
    "home-assistant",
    "lovelace",
    "hacs",
    "rss",
    "feed",
    "metro"
  ],
  "author": "Florian Schmidt",
  "license": "MIT"
}
